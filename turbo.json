{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "start:build": {
      "dependsOn": [
        "^build"
      ]
    },
    "start": {},
    "docker:beta": {
      "dependsOn": [
        "^docker:beta"
      ]
    }
  }
}